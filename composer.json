{
    "name": "maxinyugh/php-casbin",
    "description": "a powerful and efficient open-source access control library for php projects.",
    "authors": [
        {
            "name": "maxinyu",
            "email": "408567964@qq.com"
        }
    ],
    "license": "Apache-2.0",
    "keywords": [
        "casbin",
        "rbac",
        "acl",
        "authorization",
        "permission",
        "abac",
        "access control"
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/expression-language": "^3.4|^4.0",
        "s1lentium/iptools": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Casbin\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7|~6.0|~7.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Casbin\\Tests\\": "tests/"
        }
    }
}